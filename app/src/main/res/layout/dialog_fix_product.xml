<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:id="@+id/linearone"
              android:layout_width="match_parent"

              android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/txt_pls_check_information"
        android:textColor="@color/colorWhite"
        android:textSize="20sp"
        android:textStyle="bold"
        android:background="@color/colorAccent"
        android:padding="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:padding="10dp"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"

            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="@string/name"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
            <TextView
                android:id="@+id/txt_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:text="NAME"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="@string/unit"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"
                android:id="@+id/textView2"/>

            <TextView
                android:id="@+id/txt_unit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:text="UNIT"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="@string/group"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
            <TextView
                android:id="@+id/txt_group"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:text="GROUP"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="@string/price"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
            <TextView
                android:id="@+id/txt_price"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:text="PRICE"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="@string/cost"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
            <TextView
                android:id="@+id/txt_cost"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.8"
                android:text="COST"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:textStyle="bold"
                android:layout_weight="0.8"
                android:layout_height="wrap_content"
                android:text="@string/vat"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
            <TextView
                android:id="@+id/txt_vat"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:text="NAME"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:weightSum="2"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.8"
                android:textStyle="bold"
                android:layout_height="wrap_content"
                android:text="BARCODE"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"
                android:id="@+id/textView3"/>

            <TextView
                android:id="@+id/txt_barcode"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:text="barcode"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_dialogFix"
                android:padding="5dp"/>
        </LinearLayout>



    </LinearLayout>












</LinearLayout>